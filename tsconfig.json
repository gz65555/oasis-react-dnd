{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "Node",
    "declarationDir": "types",
    "declaration": true,
    "skipLibCheck": true,
    "jsx": "react",
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "emitDeclarationOnly": false,
		"outDir": "dist"
  },
  "include": ["./src"]
}
